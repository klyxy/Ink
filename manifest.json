{
  "manifest_version": 3,
  "name": "Ink",
  "version": "1.9.1",
  "description": "Schoology Addons",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": ["storage"],
  "options_page": "src/options.html",
  "content_scripts": [
    {
      "matches": ["https://app.schoology.com/*"],
      "js": ["src/content.js"],
      "css": ["src/styles.css"]
    }
  ]
}
